{
  "name": "epstore",
  "version": "1.0.0",
  "description": "Express + PostgreSQL eCommerce platform for buying/selling items",
  "main": "./src/server.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production node src/server.js",
    "start": "node src/server.js",
    "test": "cross-env NODE_ENV=test jest"
  },
  "jest": {
    "roots": [
      "test/"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h5law/epstore.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgres",
    "ecommerce"
  ],
  "author": "h5law",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/h5law/epstore/issues"
  },
  "homepage": "https://github.com/h5law/epstore#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-pg-simple": "^7.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "jest": "^28.1.2",
    "supertest": "^6.2.4"
